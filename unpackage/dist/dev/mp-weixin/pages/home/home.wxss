@charset "UTF-8";
/* 水平间距 */
/* 水平间距 */
.search-box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
}
swiper {
  height: 330rpx;
}
swiper .swiper-item,
swiper image {
  width: 100%;
  height: 100%;
}
.nav-list {
  display: flex;
  justify-content: space-around;
  margin: 15px 0;
}
.nav-list .nav-img {
  width: 128rpx;
  height: 140rpx;
}
.floor-title {
  height: 60rpx;
  width: 100%;
  display: flex;
}
.right-img-box {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.floor-img-box {
  display: flex;
  padding-left: 10rpx;
}
.sugg-list {
  padding: 0 5px;
}
.sugg-list .sugg-item {
  font-size: 12px;
  padding: 13px 0;
  border-bottom: 1px solid #efefef;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.sugg-list .sugg-item .goods-name {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-right: 3px;
}

